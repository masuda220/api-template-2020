<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="morecon.datasource.order.OrderMapper">
    <insert id="register">
        INSERT INTO 注文.注文
            (識別番号, 購入者氏名)
        VALUES(
            #{id},
            #{order.purchaser.name}
        );
    </insert>
</mapper>